{
  "name": "nuxttest",
  "version": "1.0.0",
  "engines": {
    "node": "20.x"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "yarn lint:js --fix && yarn lint:style --fix && prettier --write .",
    "lint": "yarn lint:js && yarn lint:style && prettier --check .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/test-utils": "^3.14.2",
    "@types/node": "^22",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "2",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^25.0.1",
    "jsdom-global": "^3.0.2",
    "nuxt": "^3.12.4",
    "postcss-html": "^1.6.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.5.3",
    "vitest": "^2.1.2"
  },
  "packageManager": "yarn@4.3.0"
}
