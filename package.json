    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "nuxt": "^3.8.2",
    "prettier": "^3.1.1",
    "stylelint": "^16.3.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
