{
  "name": "nuxttest",
  "version": "1.0.0",
  "engines": {
    "node": "22.x"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "yarn lint:js --fix && yarn lint:style --fix && prettier --write .",
    "lint": "yarn lint:js && yarn lint:style && prettier --check .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint": "^1.5.2",
    "@nuxt/test-utils": "^3.19.2",
